<% messages() %>
<h1><% -title %></h1>
<form action="/login" method="post" class="login">
    <div class="form-group">
        <label for="account_email">Email</label>
        <input type="email" class="form-control" id="account_email" name="account_email" required placeholder="Email">
    </div>
    <div class="form-group">
        <label for="account_password">Password</label>
        <input type="password" class="form-control" id="account_password" name="account_password" required placeholder="Password">
    </div>
    <button type="submit" class="btn btn-primary">Login</button>
    <p class="register">Don't have an account? <a href="/account/register">Register</a></p>
</form>