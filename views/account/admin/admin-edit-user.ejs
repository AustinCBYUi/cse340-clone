<%- messages() %>
<% if (errors) { %>
    <ul class="notice-bad">
    <% errors.array().forEach(error => { %>
        <li><%= error.msg %></li>
    <% }) %>
    </ul>
<% } %>
<h1><%- title %></h1>
<a href="/account/admin/admin-view-users">Back to Users</a>
<form action="/account/admin/edit/" method="post" class="add_inventory_form">
    <div class="form-group">
        <label for="account_firstname">First Name</label>
        <input type="text" id="account_firstname" name="account_firstname" value="<%= accountData.account_firstname %>" required>
    </div>
    <div class="form-group">
        <label for="account_lastname">Last Name</label>
        <input type="text" id="account_lastname" name="account_lastname" <%= account_lastname %> value="<%= accountData.account_lastname %>" required>
    </div>
    <div class="form-group">
        <label for="account_email">Email</label>
        <input type="email" id="account_email" name="account_email" <%= account_email %> value="<%= accountData.account_email %>" required>
    </div>
    <button type="submit">Update User's Account</button>
    <input type="hidden" name="account_id"
    <% if (accountData.account_id) { %> value="<%= accountData.account_id %>"
    <% } %>>
</form>